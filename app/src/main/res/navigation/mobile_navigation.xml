        tools:layout="@layout/first_exercize_student" />
    <fragment
        android:id="@+id/secondExercizeStudent"
        android:name="com.obuchalka.obuchalka.ui.exercizes.StudentExercizeFragment2"

        tools:layout="@layout/second_exercize_student" />
    <fragment
        android:id="@+id/thirdExercizeStudent"
        android:name="com.obuchalka.obuchalka.ui.exercizes.StudentExercizeFragment3"

        tools:layout="@layout/third_exercize_student" />
    <fragment
        android:id="@+id/fourthExercizeStudent"
        android:name="com.obuchalka.obuchalka.ui.exercizes.StudentExercizeFragment4"

        tools:layout="@layout/fourth_exercize_student" />
