        <action
            android:id="@+id/action_navigation_kachalka_to_fourthExercizeKachalka"
            app:destination="@id/fourthExercizeKachalka" />
        <action
            android:id="@+id/action_navigation_kachalka_to_fifthExercizeKachalka"
            app:destination="@id/fifthExercizeKachalka" />
    </fragment>

    <fragment
        android:id="@+id/navigation_kardio"
        android:name="com.obuchalka.obuchalka.ui.notifications.KardioFragment"

        tools:layout="@layout/fragment_kardio" >
        <action
            android:id="@+id/action_navigation_kardio_to_firstExercizeKardio"
            app:destination="@id/firstExercizeKardio" />
        <action
            android:id="@+id/action_navigation_kardio_to_fourthExercizeKardio"
            app:destination="@id/fourthExercizeKardio" />
        <action
            android:id="@+id/action_navigation_kardio_to_secondExercizeKardio"
            app:destination="@id/secondExercizeKardio" />
        <action
            android:id="@+id/action_navigation_kardio_to_fifthExercizeKardio"
