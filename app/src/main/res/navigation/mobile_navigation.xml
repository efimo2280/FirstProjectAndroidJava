        <action
            android:id="@+id/action_navigation_kardio_to_thirdExercizeKardio"
            app:destination="@id/thirdExercizeKardio" />
    </fragment>
    <fragment
        android:id="@+id/navigation_student"
        android:name="com.obuchalka.obuchalka.ui.StudentFragment"

        tools:layout="@layout/fragment_student" >
        <action
            android:id="@+id/action_navigation_student_to_fourthExercizeStudent"
            app:destination="@id/fourthExercizeStudent" />
        <action
            android:id="@+id/action_navigation_student_to_firstExercizeStudent"
            app:destination="@id/firstExercizeStudent" />
